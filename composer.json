{
	"name": "awesome9/templates",
	"description": "Wrapper for WordPress Filesystem and Templates.",
	"type": "library",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Awesome9",
			"email": "me@awesome9.co"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
	},
	"scripts": {
		"phpcs": "phpcs",
		"phpunit": "phpunit",
		"phpunit-coverage": "phpunit --coverage-text",
		"setup-local-tests": "bash bin/install-wp-tests.sh wordpress_test root root localhost latest"
	},
	"config": {
		"optimize-autoloader": true
	},
	"autoload": {
		"psr-4" : {
			"Awesome9\\Templates\\" : "src"
		},
		"classmap" : [ "src/" ]
	}
}
